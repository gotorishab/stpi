<odoo>
    
            <record id="view_vardhman_slider_links_form" model="ir.ui.view">
             <field name="name">vardhman.slider.links.form</field>
             <field name="model">vardhman.slider.links</field>
             <field name="arch" type="xml">
                 <form string="slider Links">
                     <sheet>
                         <group>
                             <field name="name" required="1"/>
                             <field name="url" required="1"/>
                             <field name="icon" required="1"/>
                             <field name="description"/>
                         </group>
                     </sheet>
                 </form>
             </field>
         </record>

         <record id="view_vardhman_slider_links_list" model="ir.ui.view">
             <field name="name">vardhman.slider.links.list</field>
             <field name="model">vardhman.slider.links</field>
             <field eval="8" name="priority"/>
             <field name="arch" type="xml">
                 <tree string="slider Links" editable="bottom">
                     <field name="name" required="1"/>
                     <field name="url" required="1"/>
                     <field name="description"/>
                     <field name="icon" required="1"/>
                 </tree>
             </field>
         </record>

         <record id="ir_action_slider_links" model="ir.actions.act_window">
             <field name="name">slider Links</field>
             <field name="res_model">vardhman.slider.links</field>
             <field name="view_mode">tree,form</field>
         </record>
    
    <menuitem id="menu_slider_links"
            name="slider Links"
            parent="website.menu_website_configuration"
              action="intranet_home.ir_action_slider_links"/>




