<odoo>
        <!-- ======Form View of master of File Transfer model====== -->
        <record id="file_tracking_information_form_view" model="ir.ui.view">
            <field name="name">file.tracking.information.form</field>
            <field name="model">file.tracking.information</field>
            <field name="arch" type="xml">
                <form string="File Track">
                    <field name="forwarded_by"/>
                    <field name="forwarded_date"/>
                    <field name="forwarded_to_user"/>
                    <field name="forwarded_to_dept"/>
                </form>
            </field>
        </record>

     <!-- ======Tree View of master of File Transfer model====== -->
     <record id="file_tracking_information_tree_view1" model="ir.ui.view">
            <field name="name">file.tracking.information.form</field>
            <field name="model">file.tracking.information</field>
            <field name="arch" type="xml">
                <tree>
                    <field name="forwarded_by"/>
                    <field name="forwarded_date"/>
                    <field name="forwarded_to_user"/>
                    <field name="forwarded_to_dept"/>
                    <field name="job_pos"/>
                </tree>
            </field>
     </record>

     <!-- ==========Action View of master of File model=========== -->
     <record id="file_tracking_information_action_view" model="ir.actions.act_window">
            <field name="name">file.tracking.information.action</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">file.tracking.information</field>
            <field name="view_type">form</field>
            <field name="view_mode">tree,form</field>
    </record>

</odoo>