    <record id="health_business_type_form" model="ir.ui.view">
        <field name="name">health.business.type.form</field>
        <field name="model">health.business.type</field>
        <field name="arch" type="xml">
            <form string="Business Type">
                <sheet>
                    <h1 class="d-flex">
                        Business Type
                    </h1>
                    <hr/>
                    <br/>
                    <group>
                        <group>
                            <field name="name" placeholder="Enter your  Business." required="1"/>
                        </group>
                        <group>
                            <field name="description" placeholder="Description" required="1"/>
                        </group>
                    </group>
                    <br/>
                </sheet>
            </form>
        </field>
    </record>
