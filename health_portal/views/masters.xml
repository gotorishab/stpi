    <record id="health_audit_type_master_form" model="ir.ui.view">
        <field name="name">health.audit.type.master.form</field>
        <field name="model">health.audit.type.master</field>
        <field name="arch" type="xml">
            <form string="Audit Type master">
                <sheet>
                    <h1 class="d-flex">
                        Audit Type master
                    </h1>
                    <hr/>
                    <br/>
                    <group>
                        <group>
                            <field name="audit_type" required="1"/>
                            <field name="objective" required="1" widget="many2many_tags"/>
                        </group>
                        <group>
                            <field name="frequency" required="1"/>
                            <field name="introduction" required="1"/>
                        </group>
                    </group>
                    <notebook>
                        <page name="checklist" string="'Checklist">
                            <field name="checklist_ids">
                                <tree editable="bottom">
                                    <field name="audit_type_id" invisible="1"/>
                                    <field name="checklist_item" required="1"/>
                                    <field name="sub_checklist_item" required="1"/>
                                </tree>
                            </field>
                        </page>
                    </notebook>
                </sheet>
            </form>
        </field>
    </record>
